#!/bin/bash

for DELAY in 19500 19000 18500 18000 17500 17000 16500 16000 15500 15000 14500 14000 13500 13000 12500 12000 11500 11000 10500 10000 9500 9000 8500 8000 7500 7000 6500  6000 5500 5000 4500 4000 3500 3000
do
	./bin/error_injection $DELAY & pid=$!
	./bin/naive_availability_experiment 
	kill -9 $pid
	cp time_log.csv experiment_results/naive_recovery_results/${DELAY}.csv
	
done
